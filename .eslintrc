{
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },

  "ecmaFeatures": {
    "modules": true
  },

  "parser": "babel-eslint",

  "rules": {
    "camelcase": 0,
    "curly": 0,
    "eol-last": 0,
    "no-undef": 0,
    "no-path-concat": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "quotes": [1, "single"],
    "strict": 0
  }
}